{
    "name": "gglp/yii2-app-money",
    "description": "Приложение для учёта и планирования финансов",
    "keywords": ["деньги, учёт, учет, планирование, план, бюджет, расход, доход"],
    "homepage": "http://campanelli.ru/",
    "type": "project",
    "license": "GPLv3",
    "support": {
        "issues": "https://github.com/gglp/yii2-app-money/issues?state=open",
        "source": "https://github.com/gglp/yii2-app-money"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dektrium/yii2-user": "0.9.*@dev",
        "creocoder/yii2-nested-sets": "0.9.*",
        "kartik-v/yii2-widget-select2": "@dev",
        "yii-dream-team/yii2-jstree": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
